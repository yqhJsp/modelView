<!--pages/vote/voteBoard/voteBoard.wxml-->
<!--列表循环-->
<view class='skList'>
 <block wx:for="{{list}}" wx:key="key"> 
  <view class="skOne">
    <view class='skImg'>
      <image src='{{member.userInfo.avatarUrl}}'></image>
    </view>
    <view class='skContent'>
      <view class='skfont1'>
        <label>{{member.userInfo.nickName}}</label>
      </view>
    </view>
    <view class='skLeft'>
  <label>2018-06-18</label>
  </view>
  </view>
   </block> 
  <!--end-->
</view>
<!--无数据时-->
<view class="tab-content1" wx:if="{{list.length==0}}">
  <image src='/images/mall/nodata/no.png' class='tab-image'></image>
</view>