/*图片*/

.imglist, .imglist2, .imglist3, .imglist4, .imglist5, .imglist6, .mall-padding,
.imglist8, .imglist9 {
  background-color: #fff;
  position: relative;
}

.imglist, .imglist2, .imglist3, .imglist4, .imglist6, .imglist9 {
  overflow: hidden;
  display: inline-block;
}

.imglist, .imglist2, .imglist3, .imglist4, .imglist8, .imglist9 {
  margin-bottom: 0;
}

.imglist {
  width: 210rpx;
  height: 210rpx;
  margin: 10rpx 0 0rpx 22rpx;
}

.imglist2 {
  width: 314rpx;
  margin: 20rpx 19rpx 0 23rpx;
}

.imglist3 {
  width: 670rpx;
  margin: 20rpx 20rpx 0 22rpx;
}

.imglist4 {
  color: #333;
}

.imglist8 {
  width: 50%;
  text-align: center;
  padding: 0;
  display: inline-block;
}

.imglist9 {
  width: 33.3%;
  text-align: center;
}

.imglist6 {
  width: 320rpx;
  height: 218rpx;
  margin: 10rpx 10rpx 0 22rpx;
}

.imgWidth {
  width: 95%;
  margin: 10rpx 19rpx 0 15rpx;
}

.mimg {
  width: 100%;
  height: 218rpx;
}

.imglist .mall-list-img {
  position: relative;
  overflow: hidden;
  height: 218rpx;
  width: 100%;
}

.imglist .mall-list-img image {
  width: 100%;
  height: 216rpx;
  opacity: 1;
}

.imglist2 .mall-list-img {
  position: relative;
  overflow: hidden;
  height: 198rpx;
}

.imglist2 .mall-list-img image {
  width: 100%;
  height: 198rpx;
  opacity: 1;
  border-radius: 4rpx;
}

.imglist3 .mall-list-img {
  position: relative;
  /* padding: 10rpx 0 0 0;  */
  overflow: hidden;
  height: 288rpx;
}

.imglist3 .mall-list-img image {
  width: 100%;
  opacity: 1;
  height: 288rpx;
  border-radius: 8rpx;
}

.tuwen-mb, .tuwen-mall {
  width: 100%;
  background: rgba(0, 0, 0, 0.5);
  position: absolute;
  max-width: 100%;
  display: block;
  margin: 0;
  bottom: 0;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  border-bottom-left-radius: 8rpx;
  border-bottom-right-radius: 8rpx;
}

.tuwen-mb {
  height: 60rpx;
  line-height: 55rpx;
  border-bottom-left-radius: 8rpx;
  border-bottom-right-radius: 8rpx;
}

.tuwen-mall {
  text-align: center;
}

.tuwen-mb label, .tuwen-mall label {
  color: #fff;
  font-size: 24rpx;
}

.tuwen-mb label {
  padding-left: 10rpx;
}

.imglist4  label {
  font-size: 26rpx;
}

.imglist5 {
  margin-top: 0;
  width: 100%;
}

.order_table {
  border-right: 0;
  width: 100%;
}

.orderth {
  font-size: 28rpx;
  color: #333;
}

.ordertr:not(:first-child) {
  width: 100%;
  display: flex;
  border-top: 1rpx solid #dedede;
  justify-content: space-between;
}

.imglist5 {
  text-align: center;
  border-right: 0;
  padding: 0;
}

.imglist5  image, .imglist8 image, .imglist9  image {
  width: 100%;
  display: block;
}

.mall-padding {
  width: 670rpx;
  text-align: center;
  border-right: 0;
  padding: 0px 20rpx 0 22rpx;
}

.mall-padding image {
  width: 100%;
  margin: 0;
  display: block;
}

.mall_title {
  padding: 0rpx 10rpx 0 10rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.mall_title .mall-listword {
  color: #333;
  font-size: 26rpx;
  margin-top: 10rpx;
  text-align: left;
  height: 30rpx;
}

.mall-listprice {
  padding: 10rpx 0rpx 0rpx 10rpx;
}

.mall-listprice {
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  font-size: 24rpx;
  text-align: left;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
  color: #999;
  padding: 5rpx 0rpx 0rpx 10rpx;
  height: 30rpx;
}

/*商品的列表*/

.mall-list, .mall-list2, .mall-list3 {
  background-color: #fff;
  position: relative;
  overflow: hidden;
  display: inline-block;
}

.mall-list {
  width: 31.5%;
  height: 346rpx;
  margin: 0rpx 0 0 10rpx;
}

.mall-list2 {
  width: 361rpx;
  height: 494rpx;
  margin: 0rpx 0 0 10rpx;
  overflow: hidden;
}

.mall-list3 {
  width: 24.7%;
  height: 290rpx;
}

.mall-list3:not(:first-child) {
  border-left: 1rpx solid #f5f5f5;
}

.mall-list .mall-list-img {
  position: relative;
  padding: 10rpx 10rpx 0 10rpx;
  overflow: hidden;
  height: 216rpx;
}

.mall-list .mall-list-img image {
  width: 100%;
  height: 216rpx;
  opacity: 1;
}

.shopbottom {
  padding: 0 10rpx;
}

.mall-list2 .mall-list-img {
  position: relative;
  padding: 10rpx;
  overflow: hidden;
  height: 340rpx;
}

.mall-list2 .mall-list-img image {
  width: 100%;
  height: 340rpx;
}

.mall-list3 .mall-list-img {
  position: relative;
  padding: 10rpx 10rpx 0 10rpx;
  overflow: hidden;
  height: 170rpx;
}

.mall-list3 .mall-list-img image {
  width: 100%;
  height: 170rpx;
  opacity: 1;
}

.mall_title {
  padding: 0 10rpx 0 10rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.mall_title .mall-listword {
  color: #333;
  font-size: 28rpx;
  margin-top: 10rpx;
  text-align: left;
}

.privefont {
  color: #e4393c;
}

.kucun {
  color: #8f8f94;
}

.ku {
  font-size: 20rpx;
}

.shop-prive {
  margin-top: 10rpx;
  font-size: 24rpx;
  clear: both;
  margin-left: 5rpx;
}

.fl {
  float: left;
}

.fr {
  float: right;
}

/*优惠卷*/

.couponOne {
  width: 500rpx;
  height: 206rpx;
  border: 1rpx solid #efefef;
  box-shadow: 0px 5rpx 15rpx #efefef;
  flex: 1;
  margin: 0 24rpx 0 0;
  border-radius: 8rpx;
  display: flex;
  flex-flow: row;
  background: #fff;
  position: relative;
}

.mtright {
  margin-right: 24rpx;
}

.couLeft {
  width: 100%;
  padding-left: 30rpx;
}

.couRigth {
  width: 104rpx;
  text-align: center;
  position: absolute;
  top: 0rpx;
  right: 20rpx;
}

.couprice {
  font-size: 90rpx;
  vertical-align: text-top;
}

.coutip {
  font-size: 30rpx;
  vertical-align: -10rpx;
}

.couRed {
  color: #45cdc6;
}

.coudate {
  font-size: 18rpx;
  color: #999;
}

.coubttom {
  font-size: 24rpx;
  color: #333;
  margin-top: 10rpx;
}

.getcoup, .delcoup {
  height: 104rpx;
  width: 104rpx;
  border-radius: 100%;
  font-size: 28rpx;
  text-align: center;
  margin-top: 20rpx;
  display: inline-block;
}

.getcoup {
  background: #45cdc6;
  color: white;
}

.coufont {
  width: 60rpx;
  display: inline-block;
  margin-top: 15rpx;
}

.delcoup {
  background: #f5f5f5;
  color: #333;
  line-height: 104rpx;
}
